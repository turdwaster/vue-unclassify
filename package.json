    "type": "git",
    "url": "git+https://github.com/turdwaster/vue-unclassify.git"
  },
  "main": "bin/index.js",
  "bin": {
    "vue-unclassify": "bin/index.js"
  },
  "directories": {
    "bin": "bin"
  },
  "keywords": [
    "vue",
    "vue2",
    "vue3",
    "composition",
    "script setup",
    "sfc",
    "vue-facing-decorator",
    "vue-class-component",
    "vue-property-decorator"
  ],
  "author": "Per Lind√©n <per@linden.dev> (https://github.com/turdwaster/)",
